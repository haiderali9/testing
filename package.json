{
    "name": "meraid-ap",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "set REACT_APP_SERVER_ENV=dev& craco start",
        "start:dev": "set REACT_APP_SERVER_ENV=dev-2& craco start",
        "start:qa": "set REACT_APP_SERVER_ENV=qa& craco start",
        "start:pre_prod": "set REACT_APP_SERVER_ENV=pre_prod& craco start",
        "build": "set REACT_APP_SERVER_ENV=prod& craco build",
        "build:dev": "set REACT_APP_SERVER_ENV=dev& craco build",
        "dev": "craco test",
        "eject": "react-scripts eject",
        "format": "prettier-eslint $PWD/'src/**/*.{js,json}' --write",
        "lint": "eslint src/**/*.js",
        "prettier": "prettier -c src/**/*.js",
        "test": "react-scripts test --coverage"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "@craco/craco": "6.4.0",
        "@fortawesome/fontawesome-svg-core": "1.2.36",
        "@fortawesome/free-brands-svg-icons": "5.15.4",
        "@fortawesome/free-regular-svg-icons": "5.15.4",
        "@fortawesome/free-solid-svg-icons": "5.15.4",
        "@fortawesome/react-fontawesome": "0.1.16",
        "@hookform/resolvers": "^2.8.3",
        "@reduxjs/toolkit": "1.6.2",
        "axios": "0.24.0",
        "bootstrap": "^5.1.3",
        "countries-list": "^2.6.1",
        "craco-alias": "3.0.1",
        "env-cmd": "^10.1.0",
        "formik": "2.2.9",
        "i18next": "21.4.2",
        "i18next-browser-languagedetector": "6.1.2",
        "lodash": "^4.17.21",
        "luxon": "2.1.1",
        "node-sass": "6.0.1",
        "react": "17.0.2",
        "react-app-polyfill": "^2.0.0",
        "react-bootstrap": "1.6.4",
        "react-cursor-zoom": "^1.3.0",
        "react-data-table-component": "^7.4.5",
        "react-data-table-component-extensions": "^1.5.2",
        "react-dom": "17.0.2",
        "react-hook-form": "^7.20.2",
        "react-i18next": "11.14.2",
        "react-redux": "7.2.6",
        "react-router-dom": "5.3.0",
        "react-scripts": "4.0.3",
        "react-select": "^5.2.1",
        "react-toastify": "8.1.0",
        "redux": "4.1.2",
        "redux-persist": "^6.0.0",
        "redux-thunk": "^2.4.0",
        "styled-components": "^5.3.3",
        "uuid": "8.3.2",
        "yup": "^0.32.11"
    },
    "devDependencies": {
        "@testing-library/dom": "^8.11.1",
        "@testing-library/jest-dom": "^5.15.0",
        "@testing-library/react": "^11.1.0",
        "@testing-library/user-event": "^12.1.10",
        "admin-lte": "3.1.0",
        "axios-mock-adapter": "^1.20.0",
        "eslint-config-airbnb": "19.0.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-import-resolver-alias": "1.1.2",
        "eslint-plugin-import": "2.25.3",
        "eslint-plugin-jsx-a11y": "6.5.1",
        "eslint-plugin-prettier": "4.0.0",
        "eslint-plugin-react": "7.27.0",
        "eslint-plugin-react-hooks": "4.3.0",
        "husky": "7.0.4",
        "lint-staged": "12.0.1",
        "prettier": "^2.4.1",
        "prettier-eslint": "13.0.0",
        "prettier-eslint-cli": "5.0.1",
        "pretty-quick": "^3.1.1"
    },
    "engines": {
        "node": "14.17.3",
        "npm": "6.14.13"
    },
    "lint-staged": {
        "**/*.js": [
            "eslint --fix",
            "prettier-eslint --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    }
}
